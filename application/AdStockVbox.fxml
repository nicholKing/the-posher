         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Label>
      <Label fx:id="priceLabel" prefHeight="21.0" prefWidth="120.0" text="Price">
         <font>
            <Font size="15.0" />
         </font>
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Label>
      <Label fx:id="stocksLabel" alignment="CENTER" prefHeight="30.0" prefWidth="119.0" text="Stock">
         <font>
            <Font size="15.0" />
         </font>
         <HBox.margin>
            <Insets />
