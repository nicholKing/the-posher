<HBox alignment="CENTER_LEFT" prefHeight="48.0" prefWidth="808.0" style="-fx-border-color: black;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AdStockPaneController">
   <children>
      <Label fx:id="foodnameLabel" prefHeight="21.0" prefWidth="239.0" text="Food Name">
